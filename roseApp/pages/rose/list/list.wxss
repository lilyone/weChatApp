.list-container{
	display: flex;
	height: 100%;
	padding: 0;
}
ul{
	flex: 1;
	display: flex;
	flex-wrap: wrap;
	height: 100%;
	width: 100%;
	align-items: center;
	justify-content: center;
	overflow-y: scroll;
}
li{
	box-sizing: border-box;
	list-style: none;
	width: 33.33%;
	height: 33.33%;
	box-sizing: border-box;
	display: flex;
	flex-direction: column;
	text-align: center;		
	align-items: center;
	justify-content: center;
}
li image{
	width: 50px;
	height: 50px;
	margin: 0 auto;
	margin-top: 10%;
}
li b{
	text-align: center;
	font-size: 12px;
	padding-top: 10px;
	color: #666;
}
